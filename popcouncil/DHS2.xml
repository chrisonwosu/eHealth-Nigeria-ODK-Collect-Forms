      <bind nodeset="/data/CheckAfterBirth" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y' and /data/BirthLocal!='YourHome' and /data/BirthLocal!='OtherHome'"/>
      <bind nodeset="/data/CheckHealth" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y' and /data/BirthLocal!='YourHome' and /data/BirthLocal!='OtherHome'"/>
      <bind nodeset="/data/WhoChecked" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y' and /data/CheckHealth='Y'"/>

      <bind nodeset="/data/CheckBaby" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/WhoCheckBaby" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y' and /data/CheckBaby='Y'"/>
      <bind nodeset="/data/BabyCheckLocal" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y' and /data/CheckBaby='Y'"/>
      <bind nodeset="/data/PerformBaby" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/BreastFeed" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/TimeToBreast" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y' and /data/BreastFeed='Y'"/>
      <bind nodeset="/data/Milk" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
