      <bind nodeset="/data/EverDeliver" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y'"/>
      <bind nodeset="/data/AgeDeliver" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/NoPregnant" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y'"/>
      <bind nodeset="/data/LiveBirth" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y'"/>
      <bind nodeset="/data/LiveMale" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/LiveBirth > 0"/>
      <bind nodeset="/data/LiveFemale" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y'"/>
