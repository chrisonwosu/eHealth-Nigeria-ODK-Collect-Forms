      <bind nodeset="/data/StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
      <bind nodeset="/data/EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
      <bind nodeset="/data/DeviceID" type="string" jr:preload="property" jr:preloadParams="deviceid"/>
      <bind nodeset="/data/GPSLocal" type="geopoint"/>
      <bind nodeset="/data/PlaceName" type="string"/>
      <bind nodeset="/data/HouseHead" type="string"/>
      <bind nodeset="/data/WomanName" type="string"/>
      <bind nodeset="/data/InterviewerName" type="string"/>
      <bind nodeset="/data/Result" type="select1"/>
      <bind nodeset="/data/Age" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" constraint=". &gt;= 0 and . &lt; 120"/>
      <bind nodeset="/data/AgeMenstral" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" constraint=". &gt;= 0 and . &lt; 30"/>
