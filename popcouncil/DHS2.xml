      <bind nodeset="/data/School" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'"/>
      <bind nodeset="/data/SchoolLevel" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/School='Y'"/>
      <bind nodeset="/data/SchoolCompleted" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/School='Y'"/>
      <bind nodeset="/data/Trade" type="string" relevant="/data/Result!='refused' and /data/Result!='NotHome'"/>
