      <bind nodeset="/data/CryDied" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/DeathDay1" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/DeathDay7" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/DeathDay28" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/ChildComplication" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>

      <bind nodeset="/data/Pregnant" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'"/>
      <bind nodeset="/data/MonthsPregnant" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/Pregnant='Y'"/>
      <bind nodeset="/data/LastPreg" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y'"/>
      <bind nodeset="/data/Anc" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y'"/>
      <bind nodeset="/data/NoAnc" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/Anc='Y'"/>
      <bind nodeset="/data/AncLocal" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/Anc='Y'"/>
      <bind nodeset="/data/WhoAnc" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/Anc='Y'"/>
      <bind nodeset="/data/Birth2Months" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/BirthLocal" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/BirthAssistance" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/BirthType" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
      <bind nodeset="/data/LastComplication" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y'"/>
