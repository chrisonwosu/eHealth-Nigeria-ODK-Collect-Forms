      <bind nodeset="/data/HusbandSchool" type="select" relevant="/data/Result!='refused' and /data/Result!='NotHome'"/>

      <bind nodeset="/data/PastBirths" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'"/>
