      <bind nodeset="/data/DaughtersAlive" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/EverDeliver='Y' and /data/ChildrenAlive='Y'"/>
      <bind nodeset="/data/Miscarriage" type="select1" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y'"/>
      <bind nodeset="/data/NoMiscarriages" type="int" relevant="/data/Result!='refused' and /data/Result!='NotHome'" relevant="/data/PastBirths='Y' and /data/Miscarriage='Y'"/>
